
@using cn.tdr.policeequipment.web.Models
@model GridRowModel<TableTreeHeaderModel>
@{
    var header = Model.headerModel as TableEqtTypeHeaderModel;
}

<tr>
    <th data-options="field:'@header.name.field',align:'left',width:2">@header.name.title</th>
    <th data-options="field:'@header.code.field',align:'left',width:1">@header.code.title</th>
    <th data-options="field:'@header.hits.field',align:'left',width:1">@header.hits.title</th>
    <th data-options="field:'@header.id.field',formatter:@Model.actionFormart"></th>
</tr>