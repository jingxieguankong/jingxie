
@using cn.tdr.policeequipment.web.Models
@model GridRowModel<TableHeaderModel>
@{
    var header = Model.headerModel as TableAttendanceHeaderModel;
}

<tr>
    <th data-options="field:'@header.org.field',align:'left',width:2,formatter:fmtOrg">@header.org.title</th>
    <th data-options="field:'@header.id.field',align:'left',width:2,formatter:fmtOfficer">@header.officer.title</th>
    <th data-options="field:'@header.officer.field',align:'left',width:2,formatter:fmtCode">警号</th>
    <th data-options="field:'@header.length.field',align:'left',formatter:fmtLng">@header.length.title</th>
    <th data-options="field:'@header.stime.field',align:'center',formatter:fmtDate">@header.stime.title</th>
    <th data-options="field:'@header.etime.field',align:'center',formatter:fmtDate">@header.etime.title</th>
</tr>