
@using cn.tdr.policeequipment.web.Models
@model GridRowModel<TableHeaderModel>
@{
    var header = Model.headerModel as TableOfficerHeaderModel;
}
<tr>
    @*<th data-options="field:'@header.avatar.field',align:'left',width:1,formatter:fmtAtr">@header.avatar.title</th>*@
    <th data-options="field:'@header.name.field',align:'left',width:2">@header.name.title</th>
    <th data-options="field:'@header.code.field',align:'left',width:2">@header.code.title</th>
    <th data-options="field:'@header.sex.field',align:'left',width:1">@header.sex.title</th>
    <th data-options="field:'@header.tel.field',align:'left',width:1,formatter:fmtTel">@header.tel.title</th>
    <th data-options="field:'@header.digital.field',align:'left',width:1,formatter:fmtDigital">@header.digital.title</th>
    <th data-options="field:'@header.id.field',align:'left',width:1,formatter:@Model.actionFormart"></th>
</tr>