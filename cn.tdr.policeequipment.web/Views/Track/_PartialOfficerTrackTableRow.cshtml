
@using cn.tdr.policeequipment.web.Models
@model GridRowModel<TableHeaderModel>
@{
    var header = Model.headerModel as TableOfficerTrackHeaderModel;
}

<tr>
    <th data-options="field:'@header.org.field',align:'left',width:2,formatter:fmtOrg">@header.org.title</th>
    <th data-options="field:'@header.officer.field',align:'left',width:2,formatter:fmtOfficer">@header.officer.title</th>
    <th data-options="field:'@header.officer.field',align:'left',width:2,formatter:fmtCode">警号</th>
    <th data-options="field:'@header.itime.field',align:'center',formatter:fmtDate">@header.itime.title</th>
    <th data-options="field:'@header.current.field',align:'center',width:1">@header.current.title</th>
    <th data-options="field:'@header.previous.field',align:'center',width:1">@header.previous.title</th>
</tr>